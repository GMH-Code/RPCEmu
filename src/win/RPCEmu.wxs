                  <File Id='networkAutoSenseFile' Name='EtherRPC' LongName='EtherRPCEm,ffb' DiskId='1' Source='..\riscos-progs\EtherRPCEm\!Boot\Resources\Configure\!InetSetup\AutoSense\EtherRPCEm,ffb' Vital='yes' />
                </Component>
              </Directory>
            </Directory>
          </Directory>

