            <RemoveFolder Id='ProgramMenuDir' On='uninstall' />
            <Shortcut Id="startmenudir" Directory="ProgramMenuDir" Name="rpcemudi" 
              LongName="Shortcut to RPCEmu Directory" WorkingDirectory='RPCEMUPRODUCTDIR' />

